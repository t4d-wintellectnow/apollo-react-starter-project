{
  "name": "apollo-react-starter-project",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-cache-inmemory": "1.2.9",
    "apollo-client": "2.4.1",
    "apollo-link": "1.2.2",
    "apollo-link-error": "1.1.0",
    "apollo-link-http": "1.5.4",
    "apollo-link-state": "0.4.1",
    "apollo-link-ws": "1.0.8",
    "apollo-server": "2.0.7",
    "apollo-utilities": "1.0.20",
    "graphql": "14.0.2",
    "graphql-subscriptions": "0.5.8",
    "graphql-tag": "2.9.2",
    "graphql-tools": "3.1.1",
    "node-fetch": "2.2.0",
    "react": "16.5.0",
    "react-apollo": "2.1.11",
    "react-dom": "16.5.0",
    "react-router-dom": "4.3.1",
    "react-scripts": "1.1.5",
    "subscriptions-transport-ws": "0.9.14"
  },
  "scripts": {
    "start-client": "react-scripts start",
    "build-client": "react-scripts build",
    "start-rest-server": "json-server --port 3040 --watch db.json --static node_modules/json-server/dist",
    "start-graphql-server": "concurrently \"npm run build-graphql-server\" \"npm run run-graphql-server\"",
    "run-graphql-server": "nodemon --delay 1 --watch graphql-server-dist ./graphql-server-dist/index",
    "build-graphql-server": "chokidar \"graphql-server/**/*.js\" -c \"babel graphql-server -d graphql-server-dist\""
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.7.0",
    "chokidar-cli": "1.2.1",
    "concurrently": "4.0.1",
    "json-server": "0.14.0",
    "nodemon": "1.18.4"
  }
}
